        script(src='https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js')
        script(src='./scripts/landing.js')
        title ySpace | YourSpace
    body
      main(class="main")
        div(class="container")
          div(class="profile-info")
            div(class="profile-picture-border")
